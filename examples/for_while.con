mod Example {
    fn main() -> i64 {
        return sum_to_twice(4);
    }

    fn sum_to_twice(limit: i64) -> i64 {
        let mut result: i64 = 0;

        let n: i64 = 1;
        for (n <= limit) {
            result = result + n;
            n = n + 1;
        }

        return result;
    }
}
