mod Example {
    fn main() -> i32 {
        let mut array: [i32; 4] = [1, 9, 3, 4];
        let mut nested_array: [[i32; 2]; 2] = [[1, 2], [9, 9]];

        array[1] = 2;
        nested_array[1] = [3, 4];

        let a: i32 = array[1];
        let b: i32 = nested_array[1][0];

        return a + b;
    }
}
